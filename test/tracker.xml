<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../DDLSchema/DDLSchema.xsd">
  <SpecParSection label="regions.xml" eval="true">
    <SpecPar name="tracker-dead">
      <PartSelector path="//TRAK" />
      <Parameter name="CMSCutsRegion" value="TrackerDeadRegion" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="10*cm"/>
      <Parameter name="ProdCutsForPositrons" value="10*cm"/>
      <Parameter name="ProdCutsForGamma" value="10*cm"/>
  </SpecPar>
    <SpecPar name="tracker-sens">
      <PartSelector path="//SCD1" />
      <PartSelector path="//SCG1" />
      <PartSelector path="//SCD2" />
      <PartSelector path="//SCG2" />
      <PartSelector path="//SCS3" />
      <PartSelector path="//SWD1" />
      <PartSelector path="//SWS3" />
      <PartSelector path="//SWS4" />
      <PartSelector path="//SWD2" />
      <PartSelector path="//SWG1" />
      <PartSelector path="//SWG2" />
      <PartSelector path="//GWD1" />
      <PartSelector path="//GWG1" />
      <PartSelector path="//GWD2" />
      <PartSelector path="//GWG2" />
      <PartSelector path="//GWS3" />
      <PartSelector path="//GWS4" />
      <PartSelector path="//GWS5" />
      <PartSelector path="//GWS6" />
      <PartSelector path="//GCS3" />
      <PartSelector path="//GCS4" />
      <PartSelector path="//GCD5" />
      <PartSelector path="//GCG5" />
      <PartSelector path="//GCS6" />
      <PartSelector path="//GCS7" />
      <PartSelector path="//GCD1" />
      <PartSelector path="//GCG1" />
      <PartSelector path="//GCD2" />
      <PartSelector path="//GCG2" />
      <Parameter name="CMSCutsRegion" value="TrackerSensRegion" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="0.1*mm"/>
      <Parameter name="ProdCutsForPositrons" value="0.1*mm"/>
      <Parameter name="ProdCutsForGamma" value="0.1*mm"/>
  </SpecPar>
 <SpecPar name="tracker-dead-pixel">
      <PartSelector path="//PXLX" />
      <Parameter name="CMSCutsRegion" value="TrackerPixelDeadRegion" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="1*mm"/>
      <Parameter name="ProdCutsForPositrons" value="1*mm"/>
      <Parameter name="ProdCutsForGamma" value="1*mm"/>
  </SpecPar>

<SpecPar name="tracker-sens-pixel">
      <PartSelector path="//PXBD" />
      <PartSelector path="//PXFD" />
      <Parameter name="CMSCutsRegion" value="TrackerPixelSensRegion" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="0.01*mm"/>
      <Parameter name="ProdCutsForPositrons" value="0.01*mm"/>
      <Parameter name="ProdCutsForGamma" value="0.01*mm"/>
  </SpecPar>



    <SpecPar name="BeamPipe">
      <PartSelector path="//BTUB" />
      <Parameter name="CMSCutsRegion" value="BeamPipe" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="0.01*mm"/>
      <Parameter name="ProdCutsForPositrons" value="0.01*mm"/>
      <Parameter name="ProdCutsForGamma" value="0.01*mm"/>
  </SpecPar>

  <SpecPar name="BeamPipeNotBeryllium">
      <PartSelector path="//BVTB" />
      <Parameter name="CMSCutsRegion" value="BeamPipeInternal" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="10000*mm"/>
      <Parameter name="ProdCutsForPositrons" value="10000*mm"/>
      <Parameter name="ProdCutsForGamma" value="10000*mm"/>
  </SpecPar>

  <SpecPar name="TIB_Ledge_Side_Prod_Cuts">
      <PartSelector path="//SRAN" />
      <Parameter name="CMSCutsRegion" value="TIB_Ledge_Side_ProdCuts" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="5*mm"/>
      <Parameter name="ProdCutsForPositrons" value="5*mm"/>
      <Parameter name="ProdCutsForGamma" value="5*mm"/>
  </SpecPar>

  </SpecParSection>
</DDDefinition>


